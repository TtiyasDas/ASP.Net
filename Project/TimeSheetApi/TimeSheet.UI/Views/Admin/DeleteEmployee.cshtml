@model TimeSheet.Entity.Models.Employee

<form enctype="multipart/form-data">
    <h2>Delete Employee</h2>

    <hr />
    @if (ViewBag.status == "Ok")
    {
        <div class="alert alert-success">
            @ViewBag.message
        </div>
    }
    else if (ViewBag.status == "Error")
    {
        <div class="alert alert-danger">
            @ViewBag.message
        </div>
    }

</form>